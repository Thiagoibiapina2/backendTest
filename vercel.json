{
  "version": 2,
  "builds": [
    {
      "src": "./server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth",
      "dest": "./server.ts"
    },
    {
      "src": "/api/User",
      "dest": "./server.ts"
    },
    {
      "src": "/api/Post",
      "dest": "./server.ts"
    },
    {
      "src": "/api/Post/:id",
      "dest": "./server.ts"
    },
    {
      "src": "/api/Post/:id",
      "dest": "./server.ts"
    },
    {
      "src": "/api/Post",
      "dest": "./server.ts"
    },
    {
      "src": "/api/Post/:id",
      "dest": "./server.ts"
    }
  ]
}
